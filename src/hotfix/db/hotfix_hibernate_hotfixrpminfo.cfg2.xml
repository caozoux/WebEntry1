<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE hibernate-mapping PUBLIC 
       "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
       "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  <hibernate-mapping>
      <!-- name代表的是实体类名，table代表的是表名 -->
    <class name="hotfix.db.hotfix_rpmpackage_bean" table="rpmpackages_debug">
        <!-- 主键 -->
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        
        <!-- 字段 -->
        <property name="akid" type="java.lang.String">
            <column name="akid" />
        </property>
        
      	<!-- 字段 -->
        <property name="kernel" type="java.lang.String">
            <column name="kernel" />
        </property>
        
        <!-- 字段 -->
        <property name="rpmname" type="java.lang.String">
            <column name="rpmname" />
        </property>
        
         <!-- 字段 -->
        <property name="downlink" type="java.lang.String">
            <column name="downlink" />
        </property>
        
         <!-- 字段 -->
        <property name="testlink" type="java.lang.String">
            <column name="testlink" />
        </property>
    
    </class>
  </hibernate-mapping>
