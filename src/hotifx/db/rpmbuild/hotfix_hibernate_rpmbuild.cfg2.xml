<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE hibernate-mapping PUBLIC 
       "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
       "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  <hibernate-mapping>
   <class name="hotifx.db.rpmbuild.hotfix_hibernate_rpmbuild_bean" table="hotfix_build_process">
        <!-- 主键 -->
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        
        <property name="status" type="int">
            <column name="status" />
        </property>
        
        <!-- 字段 -->
        <property name="akid" type="java.lang.String">
            <column name="akid" />
        </property>
        
      	<!-- 字段 -->
        <property name="debuglog" type="java.lang.String">
            <column name="debuglog" />
        </property>
    
        <!-- 字段 -->
        <property name="versionlist" type="java.lang.String">
            <column name="versionlist" />
        </property>  
        
        <!-- 字段 -->
        <property name="completelist" type="java.lang.String">
            <column name="completelist" />
        </property>  
        
        <!-- 字段 -->
        <property name="currenbuild" type="java.lang.String">
            <column name="currenbuild" />
        </property>
        
        <property name="kerneltype" type="int">
            <column name="kerneltype" />
        </property>
    </class>
  </hibernate-mapping>
